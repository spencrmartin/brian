{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Brian app default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-stdin-write",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "brian-backend",
          "sidecar": true
        }
      ]
    }
  ]
}
